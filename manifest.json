{
    "name": "Roadwork in Wonderland",
    "description": "No going down that rabbit hole for you",
    "version": "0.1",
    "manifest_version": 3,
    "permissions": [
        "history",
        "tabs"
    ],

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["main.js"]
        }
    ],

    "action": {
        "default_popup": "popup/index.html"
    }
}